<Bindings>
    <Binding name="MouseLook_mode_toggle" description="Enable/Disable MouseLook Lock mode" header="MouseLook_Lock">
        MouseLook_Toggle()
    </Binding>

    <Binding name="MouseLook_momentary" description="Brings up the pointer while held down" runOnUp="true" hidden="false">
        MouseLook_Momentary(keystate)
    </Binding>
</Bindings>
